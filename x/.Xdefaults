
Xft.autohint: true
Xft.lcdfilter:  lcddefault

Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight


!URxvt*font:                 xft:Consolas-11
!URxvt*font:                 xft:DejaVu\ Sans\ Mono\ for\ Powerline-10
!URxvt*font:                 xft:Source\ Code\ Pro\ for\ Powerline-10:style=Light
!URxvt*font:                 xft:Roboto\ Mono\ for\ Powerline:size=10
URxvt*font:                 xft:Hack:size=10
!URxvt*font:                 xft:Inconsolata:size=12:style=dz
!URxvt*font:                 xft:Terminus:size:10
!URxvt*font:                 xft:Mononoki:size:10
!URxvt*font: -*-terminus-*-*-*-*-12-*-*-*-*-*-*-9

!URxvt*background: #1c1f26
! URxvt*foreground: white
URxvt*scrollBar: false
URxvt*perl-ext-common:  default,matcher,clipboard
URxvt*urlLauncher:      /usr/bin/x-www-browser
URxvt*matcher.button:   1
URxvt*buffered: false
URxvt*transparent: false
URxvt*shading: 0
URxvt.letterSpace: -1


